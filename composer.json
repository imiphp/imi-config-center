{
    "name": "imiphp/imi-config-center",
    "type": "library",
    "license": "MIT",
    "description": "imi 框架配置中心组件。imi configuration center.",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~3.4.0",
        "phpstan/phpstan": "~1.8.0",
        "phpunit/phpunit": ">=9",
        "rector/rector": "^0.12.10",
        "imiphp/imi": "~2.1.0",
        "imiphp/imi-swoole": "~2.1.0",
        "imiphp/imi-workerman": "~2.1.0",
        "swoole/ide-helper": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Imi\\ConfigCenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imi\\ConfigCenter\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}